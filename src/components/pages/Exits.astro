---
import NotLogged from '@dashboard/NotLogged.astro'
import ExitsForm from '@exits/ExitsForm.astro'
import Dashboard from '@layouts/Dashboard.astro'

const userCookie = Astro.cookies.get('user')
const user = userCookie ? JSON.parse(decodeURIComponent(userCookie?.value)) : null
---

<Dashboard title='Gastos'>
  {user ? <ExitsForm /> : <NotLogged />}
</Dashboard>
