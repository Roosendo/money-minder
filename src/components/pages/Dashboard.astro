---
import MainPage from '@dashboard/MainPage.astro'
import NotLogged from '@dashboard/NotLogged.astro'
import DashboardLayout from '@layouts/Dashboard.astro'

const userCookie = Astro.cookies.get('user')
const user = userCookie ? JSON.parse(decodeURIComponent(userCookie?.value)) : null
---

<DashboardLayout title='Dashboard'>
  {user ? <MainPage /> : <NotLogged />}
</DashboardLayout>
