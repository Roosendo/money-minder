---
import NotLogged from '@dashboard/NotLogged.astro'
import Dashboard from '@layouts/Dashboard.astro'
import RemindersForm from '@reminders/RemindersForm.astro'
import { getSession } from 'auth-astro/server'

const session = await getSession(Astro.request)
---

<Dashboard title='Recordatorios'>
	{session ? <RemindersForm /> : <NotLogged />}
</Dashboard>
